		<block id="Blinn" position_x="10" position_y="1" author="rconstruct">
			<input name="Ks" type="float" storage="varying" value="0.8" description="Specular light coefficient">
				<connection parent="Divide_3" output="val"/>
			</input>
			<input name="size" type="float" storage="varying" value="15" description="Specular size.">
				<connection parent="Multiply_7" output="val"/>
			</input>
			<input name="Cspec" type="color" storage="varying" value="1" description="Specular color"/>
			<input name="N" type="vector" storage="varying" value="normalize(N)" description="Surface normal">
				<connection parent="Normalize_3" output="val"/>
			</input>
			<input name="I" type="vector" storage="varying" value="-normalize(I)" description="Direction of viewer"/>
			<output name="Ci" type="color" storage="varying" description="Shaded color"/>
			<code>

        $(Ci) = $(Ks) * $(Cspec) * blinn( $(N), $(I), $(size) );

										</code>
			<include>shadingmodels.h									</include>
			<usage>Blinn specular term. This is meant to be used in a network with an ambient and a diffuse term. Add Cs/Os as needed. A good starting point for experimentation might be to use the Oren-Nayar diffuse term.									</usage>
		</block>
		<block id="Bump" position_x="10" position_y="-2" author="rconstruct">
			<input name="P" type="point" storage="varying" value="P" description="The surface point to be bumped"/>
			<input name="Dir" type="vector" storage="varying" value="normalize(N)" description="The surface normal" type_parent="NN"/>
			<input name="Scale" type="float" storage="varying" value="0.5" description="Amplitude of the bumping">
				<connection parent="Multiply" output="val"/>
			</input>
			<output name="NN" type="vector" storage="varying" description="The new surface normal"/>
			<code>
	$(NN) = normalize( calculatenormal( $(P) + $(Dir)*$(Scale) ) );
										</code>
			<include/>
			<usage>This is exactly like the displace function except that the new surface position is not computed.
										</usage>
		</block>
		<block id="CellContour" position_x="3" position_y="-4" author="rconstruct">
			<input name="p1" type="point" storage="varying" value="0" description="First point position">
				<connection parent="Add_3" output="val"/>
			</input>
			<input name="p2" type="point" storage="varying" value="1" description="Second point position">
				<connection parent="Add_4" output="val"/>
			</input>
			<input name="f1" type="float" storage="varying" value="0" description="Primary feature">
				<connection parent="Divide" output="val"/>
			</input>
			<input name="f2" type="float" storage="varying" value="1" description="Secondary feature">
				<connection parent="Divide_2" output="val"/>
			</input>
			<input name="lthick" type="float" storage="varying" value="5" description="Contour line thickness"/>
			<input name="P" type="point" storage="varying" value="P" description="Surface point"/>
			<output name="val" type="float" storage="varying" description="Output value"/>
			<code>
		$(val) = step( $(lthick) * (
					distance( $(p1), $(p2) ) / (
					distance( $(p1), $(P) ) + distance( $(P), $(p2) ) ) ) ,
					$(f2) - $(f1) );
										</code>
			<include>shrimp_helpers.h									</include>
			<usage/>
		</block>
