<shrimp type="function" name="VoronoiKM2D2F" description="2D Voronoi noise, 2 features, Karlsruhe (Moscow) metric" author="rconstruct">
	<input name="ss" description="s coordinate" type="float" default="s"/>
    <input name="tt" description="t coordinate" type="float" default="t"/>
	<input name="jitter" description="Jitter" type="float" default="1"/>
    <output name="f1" description="feature 1" type="float"/>
    <output name="spos1" description="s position 1" type="float"/>
    <output name="tpos1" description="t position 1" type="float"/>
    <output name="f2" description="feature 2" type="float"/>
    <output name="spos2" description="s position 2" type="float"/>
    <output name="tpos2" description="t position 2" type="float"/>
	<include>shrimp_extrapatterns.h</include>
	<rsl_code>
	/* initialize */
	$(f1) = 0; $(spos1) = 0; $(tpos1) = 0;
	$(f2) = 0; $(spos2) = 0; $(tpos2) = 0;
    voronoi_km_f1f2_2d( $(ss), $(tt), $(jitter), $(f1), $(spos1),
								 $(tpos1), $(f2), $(spos2), $(tpos2));
	</rsl_code>
	<usage>2D Voronoi cell noise, 2 feature version, using Karlsruhe (Moscow) distance metric.</usage> 
</shrimp>
