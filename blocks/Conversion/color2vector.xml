<shrimp type="function" name="Color2Vector" description="Color to vector conversion" author="rconstruct">
    <input name="in" description="Input color" type="color" default="0"/>
    <output name="out" description="Output value" type="vector"/>
	<code>
	    $(out) = vector( comp( $(in), 0), comp( $(in), 1), comp( $(in), 2) );
	</code>
</shrimp>
