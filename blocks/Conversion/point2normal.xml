<shrimp type="function" name="Point2Normal" description="Point to normal conversion">
    <input name="in" description="Input value" type="point" default="0"/>
    <output name="out" description="Output value" type="normal"/>
	<code>
	    $(out) = normal( comp( $(in), 0), comp( $(in), 1), comp( $(in), 2) );
	</code>
</shrimp>
