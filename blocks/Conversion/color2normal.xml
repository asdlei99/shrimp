<shrimp type="function" name="Color2Normal" description="Color to normal conversion" author="rconstruct">
    <input name="in" description="Input color" type="color" default="0"/>
    <output name="out" description="Output value" type="normal"/>
	<rsl_code>
	    $(out) = normal( comp( $(in), 0), comp( $(in), 1), comp( $(in), 2) );
	</rsl_code>
</shrimp>
