<shrimp type="function" name="Voronoi2D1F" description="2D Voronoi noise, 1 features." author="rconstruct">
	<input name="ss" description="s coordinate" type="float" default="s"/>
    <input name="tt" description="t coordinate" type="float" default="t"/>
	<input name="jitter" description="Jitter" type="float" default="1"/>
	<input name="dtype" description="Minkowski P parameter, see help" type="float" default="2"/>
    <output name="f1" description="feature 1" type="float"/>
    <output name="spos1" description="s position 1" type="float"/>
    <output name="tpos1" description="t position 1" type="float"/>
	<include>fractal.h</include>
	<code>
    voronoi_f1_2d( $(ss), $(tt), $(jitter), $(dtype),$(f1), $(spos1), $(tpos1));
	</code>
	<usage>2D Voronoi cell noise, 1 feature version. Uses Minkowski distanc emetric, so when dtype = 1, distance metric is Euclidian, when dtype = 2, distance metric = Manhattan.</usage> 
</shrimp>
