<shrimp type="function" name="Voronoi3D1F" description="3D Voronoi noise, 1 feature." author="rconstruct">
	<input name="p" description="point to sample Voronoi at" type="point" default="P"/>
	<input name="jitter" description="Jitter" type="float" default="1"/>
	<input name="dtype" description="Minkowski P paramater, see help" type="float" default="2"/>
    <output name="f1" description="feature 1" type="float"/>
	<output name="pos1" description="position 1" type="point"/>
	<include>fractal.h</include>
	<code>
    voronoi_f1_3d( $(p), $(jitter), $(dtype), $(f1), $(pos1));
	</code>
	<usage>3D Voronoi cell noise, 1 features version. Uses Minkowski distance metric, so when dtype = 1, distance metric is Euclidian, when dtype = 2, distance metric = Manhattan.</usage> 
</shrimp>
