<shrimp type="function" name="Cross" description="Compute cross product" author="rconstruct">
	<input name = "A" description="1st Input" type="vector point normal" default="0"/>
	<input name = "B" description="2nd Input" type="vector point normal" default="1"/>
	<output name="val" description="val = A ^ B" type="vector point normal"/>
	<code>
	$(val) = $(A)^$(B);
	</code>
</shrimp>
