<shrimp type="function" name="Degrees" description="Convert radians to degrees" author="rconstruct">
	<input name="rad" description="Input angle in radians" type="float" default="1"/>
	<output name="deg" description="Angle converted to degrees" type="float"/>
	<rsl_code>
	$(deg) = degrees( $(rad) );
	</rsl_code>
</shrimp>
