<shrimp type="function" name="Min" description="Return the smallest input" author="rconstruct">
	<input name="a" description="1st input" type_parent="out" type="float point vector normal color" default="0"/>
	<input name="b" description="2nd or subsequent inputs" type_parent="out" type="float point vector normal color" multi="," default="0"/>
	<output name="out" description="out = the smallest input" type="float point vector normal color"/>
	<rsl_code>
		$(out) = min( $(a), $(b) );
	</rsl_code>
</shrimp>
