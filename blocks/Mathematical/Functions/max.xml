<shrimp type="function" name="Max" description="Return the largest input" author="rconstruct">
	<input name="a" description="1st input" type_parent="out" type="float point vector normal color" default="0"/>
	<input name="b" description="2nd or subsequent inputs" type_parent="out" type="float point vector normal color" multi="," default="1"/>
	<output name="out" description="out = the largest input" type="float point vector normal color"/>
	<code>
		$(out) = max( $(a), $(b) );
	</code>
</shrimp>
