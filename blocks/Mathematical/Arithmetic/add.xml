<shrimp type="function" name="Add" description="Addition block" author="rconstruct">
	<input name="Addition" description="multiple input" multi="+" type="float color point vector normal" type_parent="val" default="1"/>
	<output name="val" description="sum of all inputs" type="float color point vector normal"/>
	<code>
	$(val) = $(Addition);
	</code>
	<usage>All inputs must be of the same type. All connected inputs are added together.</usage>
</shrimp>
