<shrimp type="function" name="AND" description="Boolean AND function">
	<input name = "A" description="1st value"/>
	<input name = "B" description="2nd value"/>
	<output name="out" description="The intersection of A and B"/>
	<include>shrimp_util.h</include>
	<code>
	$(out) = intersection( $(A), $(B) );
	</code>
	<usage>This block computes the logical AND of the two inputs. The inputs should be in the range 0-1.
	</usage>
</shrimp>