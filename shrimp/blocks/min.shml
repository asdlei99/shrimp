<shrimp type="function" name="Min" description="Return the smallest input">
	<input name="a" description="1st input" type="float point vector normal color"/>
	<input name="b" description="2nd or subsequent inputs" type="float point vector normal color" multi="true" separator=", "/>
	<output name="out" description="out = the smallest input" type="float point vector normal color"/>
	<code>
	$(out) = min( $(a), $(b) );
	</code>
</shrimp>