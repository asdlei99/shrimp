<shrimp type="function" name="SUB" description="Boolean difference">
	<input name = "A" description="1st value"/>
	<input name = "B" description="2nd value"/>
	<output name="out" description="Performs the logical operation A-B"/>
	<include>shrimp_util.h</include>
	<code>
	$(out) = difference( $(A), $(B) );
	</code>
	<usage>This block computes the logical operation A-B. The inputs should be in the range 0-1.
	</usage>
</shrimp>